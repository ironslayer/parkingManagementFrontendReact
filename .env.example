# ==========================================
# CONFIGURACIÓN DE LA API
# ==========================================
VITE_API_BASE_URL=http://localhost:8080/api

# ==========================================
# CONFIGURACIÓN DE AUTENTICACIÓN
# ==========================================
VITE_JWT_SECRET_KEY=your-secret-key-here
VITE_TOKEN_STORAGE_KEY=parking_auth_token
VITE_REFRESH_TOKEN_STORAGE_KEY=parking_refresh_token

# ==========================================
# CONFIGURACIÓN DE LA APLICACIÓN
# ==========================================
VITE_APP_NAME=Sistema de Gestión de Parqueadero
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# ==========================================
# CONFIGURACIÓN DE LOGGING
# ==========================================
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# ==========================================
# CONFIGURACIÓN DE CARACTERÍSTICAS
# ==========================================
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_WEBSOCKETS=true
VITE_ENABLE_PWA=false

# ==========================================
# CONFIGURACIÓN DE TERCEROS
# ==========================================
# Ejemplo para integración con pasarelas de pago
# VITE_PAYMENT_GATEWAY_API_KEY=your-payment-gateway-key
# VITE_MAPS_API_KEY=your-maps-api-key
